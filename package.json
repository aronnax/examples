{
  "name": "pint-notes",
  "version": "0.0.1",
  "description": "Test game",
  "main": "dist/ping.js",
  "scripts": {
    "clean": "rm -r dist/*",
    "bundle": "npm run clean && browserify src/main.js -t babelify -o dist/main.js",
    "compile": "npm run clean && browserify src/main.js -t babelify -t uglifyify -o dist/main.min.js",
    "lint": "eslint src/*.js",
    "test": "npm run unit-test && npm run integration-test",
    "unit-test": "testem ci -f test/unit_testem.json -l phantomjs",
    "watch-bundle": "watch-run -p './src/*.js' npm run bundle"
  },
  "keywords": [
    "gaming",
    "performance"
  ],
  "author": "Marco Segreto <msegreto@miceover.com>",
  "license": "MIT",
  "devDependencies": {
    "aronnax-looping": "0.0.3",
    "aronnax-pooling": "^0.5.6",
    "babel": "^5.1.13",
    "babel-eslint": "^3.0.1",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "eslint": "^0.19.0",
    "function-bind": "^1.0.2",
    "phantomjs": "^1.9.16",
    "sinon": "^1.14.1",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watch-run": "^1.2.2",
    "watchify": "^3.2.1"
  }
}
